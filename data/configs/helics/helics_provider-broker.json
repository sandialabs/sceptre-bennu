{
    "name": "bennu",
    "request_time": "max",
    "terminate_on_error": true,
    "only_update_on_change": true,
    "subscriptions": [
        {
            "key": "helics-test-ep-server/bus-1.voltage",
            "type": "double"
        },

        {
            "key": "helics-test-ep-server/generator-1_bus-1.active",
            "type": "bool"
        },

        {
            "key": "helics-test-ep-server/load-1_bus-1.active",
            "type": "bool"
        },

        {
            "key": "helics-simulink-provider/Pump1.flow",
            "type": "double"
        },

        {
            "key": "helics-simulink-provider/Pump1.active",
            "type": "double"
        },
    ],
    "endpoints": [
        {
            "name": "bus-1.volatge",
            "destination": "helics-test-ep-server/bus-1.voltage"
        },

        {
            "name": "generator-1_bus-1.active",
            "destination": "helics-test-ep-server/generator-1_bus-1.active"
        },

        {
            "name": "load-1_bus-1.active",
            "destination": "helics-test-ep-server/load-1_bus-1.active"
        },

        {
            "name": "Pump1.flow",
            "destination": "helics-simulink-provider/Pump1.flow"
        },

        {
            "name": "Pump1.active",
            "destination": "helics-simulink-provider/Pump1.active"
        },
    ]
}
